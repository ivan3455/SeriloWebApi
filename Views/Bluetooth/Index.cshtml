<!DOCTYPE html>
<html>
<head>
    <title>Connected Bluetooth Devices</title>
    <style>
        ul {
            list-style-type: none;
            padding: 0;
        }

        li {
            margin-bottom: 5px;
        }
    </style>
</head>
<body>
    <h1>Connected Bluetooth Devices</h1>
    <ul>
        @foreach (var device in (List<string>)ViewData["ConnectedDevices"])
        {
            <li>@device</li>
        }
    </ul>
</body>
</html>
